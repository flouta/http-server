server{

	server_name name;

	listen 10.11.4.10:8080;

	error_page 404 /error_page.html;

	client_body_size_max 100;

	location / {

		allow_methods GET POST DELETE;

		redirect 301 /redirect.html; 

		root /path/to/dir;
		
		autoindex on;

		default /default.html;

		cgi .php /bin/php;

		upload /goinfre;

	}

}

server{

	server_name name;

	listen localhost;

	error_page 404 /error_page.html;

	client_body_size_max 100;

	location / {

		allow_methods GET POST DELETE;

		redirect 301 /redirect.html; 

		root /path/to/dir;
		
		autoindex on;

		default /default.html;

		cgi .php /bin/php;

		upload /goinfre;

	}

}
